<?xml version="1.0" encoding="UTF-8"?><analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
	<frameworkImplementation>org.apache.uima.java</frameworkImplementation>
	<primitive>true</primitive>
	<annotatorImplementationName>com.ctapweb.feature.featureAE.CohesiveComplexityAE</annotatorImplementationName>
	<analysisEngineMetaData>
		<name>Cohesive Complexity Feature: Adversative and Concessive Connectives per Token (Eisenberg for German)</name>
		<description>Calculates the cohesive complexity of the text. This feature calculates the adversative and concessive connectives per token, listed by Eisenberg for German.
			Definition: # of adversative and concessive connectives / # of token

			AAE dependency:
			SentenceAnnotator.xml
			TokenAnnotator.xml
			NConnectives_Breindl_Temporal_Feature.xml
			POSDensity_NonPunctuationWords.xml</description>
		<version>1.0</version>
	<vendor>Zarah Weiss</vendor>
	<configurationParameters>
		<configurationParameter>
			<name>aeID</name>
			<type>Integer</type>
			<multiValued>false</multiValued>
			<mandatory>true</mandatory>
		</configurationParameter>
		<configurationParameter>
			<name>numerator</name>
			<description/>
			<type>String</type>
			<multiValued>false</multiValued>
			<mandatory>true</mandatory>
		</configurationParameter>
		<configurationParameter>
			<name>LanguageCode</name>
			<type>String</type>
			<multiValued>false</multiValued>
			<mandatory>false</mandatory>
		</configurationParameter>
		<configurationParameter>
			<name>denominator</name>
			<description/>
			<type>String</type>
			<multiValued>false</multiValued>
			<mandatory>true</mandatory>
		</configurationParameter>
	</configurationParameters>
	<configurationParameterSettings>
		<nameValuePair>
			<name>numerator</name>
			<value>
				<string>nEisenberg_AC</string>
			</value>
		</nameValuePair>
		<nameValuePair>
			<name>denominator</name>
			<value>
				<string>nWords</string>
			</value>
		</nameValuePair>
	<nameValuePair><name>LanguageCode</name><value><string>DE</string></value></nameValuePair><nameValuePair><name>aeID</name><value><integer>25731475</integer></value></nameValuePair></configurationParameterSettings>
	<typeSystemDescription>
		
	<imports/></typeSystemDescription>
	<typePriorities/>
	<fsIndexCollection/>
	<capabilities>
		<capability>
			<inputs>
				<type allAnnotatorFeatures="true">com.ctapweb.feature.type.NConnectives</type>
        <type allAnnotatorFeatures="true">com.ctapweb.feature.type.NToken</type>
			</inputs>
			<outputs>
				<type allAnnotatorFeatures="true">com.ctapweb.feature.type.CohesiveComplexity</type>
			</outputs>
			<languagesSupported>
				<language>DE</language>
				<!-- add new language here -->
			</languagesSupported>
		</capability>
	</capabilities>
	<operationalProperties>
		<modifiesCas>true</modifiesCas>
		<multipleDeploymentAllowed>true</multipleDeploymentAllowed>
		<outputsNewCASes>false</outputsNewCASes>
	</operationalProperties>
	</analysisEngineMetaData>
	<resourceManagerConfiguration/>
</analysisEngineDescription>